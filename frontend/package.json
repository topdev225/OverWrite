    "stage": "cp .env.stage .env && webpack-dev-server --inline --progress --hot --host 0.0.0.0 --port 60602 --config build/webpack.dev.conf.js",
    "prod": "cp .env.prod .env && webpack-dev-server --inline --progress --hot --host 0.0.0.0 --port 60602 --config build/webpack.dev.conf.js",
    "test": "jest",
    "e2e": "cypress run"
  },
