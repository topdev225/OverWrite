  "scripts": {
    "start": "node server.js",
    "lint": "eslint --ext .js,.vue src",
    "build": "node build/build.js",
    "local": "cp .env.local .env && webpack-dev-server --inline --progress --hot --config build/webpack.dev.conf.js",
    "local_docker": "cp .env.local .env && webpack-dev-server --inline --progress --hot --host 0.0.0.0 --port 60602 --config build/webpack.dev.conf.js",
